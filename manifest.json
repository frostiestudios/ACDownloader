{
    "browser_action": {
        "default_icon": {
          "19": "button/geo-19.png",
          "38": "button/geo-38.png"
        },
        "default_title": "Whereami?"
      },
      
  "manifest_version": 2,
  "name": "AC Downloader",
  "version": "1.0",

  "description": "Adds a red border to all webpages matching mozilla.org.",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.mozilla.org/*"],
      "js": ["program.js"]
    }
  ]

}
